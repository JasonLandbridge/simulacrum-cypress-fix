{
  "name": "@simulacrum/github-api-simulator",
  "version": "0.1.0",
  "private": false,
  "description": "Provides common functionality to frontend app and plugins.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "backstage": {
    "role": "backend"
  },
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md",
    "schema"
  ],
  "scripts": {
    "lint": "eslint src bin test",
    "prepack": "npm run build",
    "build": "tsc && copyfiles -f src/schema/schema.docs*.graphql dist/schema",
    "start": "PORT=3300 nodemon",
    "watch": "NODE_ENV=development yarn concurrently \"tsc -b --watch\" \"yarn generate:watch\"",
    "generate": "graphql-codegen",
    "generate:watch": "yarn generate --watch",
    "test": "echo N/A.....yet"
  },
  "dependencies": {
    "@frontside/graphgen": "^1.7.0",
    "@graphql-yoga/node": "^2.13.13",
    "assert-ts": "^0.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "lodash.omit": "^4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/graphql-modules-preset": "^2.3.8",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-resolvers": "^2.6.1",
    "@types/express": "^4.17.14",
    "@types/lodash.omit": "^4.5.7",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "@graphql-codegen/cli": "2.x.x"
  },
  "engines": {
    "node": "14 || 16"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
